/dts-v1/;

/ {
	#address-cells = <0x02>;
	#size-cells = <0x02>;
	compatible = "riscv-minimal-nommu";
	model = "riscv-minimal-nommu,qemu";

	chosen {
	//	bootargs = "earlycon=uart8250,mmio,0x10000000,3000000 console=ttyS0 debug memtest=0";
    bootargs = "earlycon=uart8250,mmio,0x10000000,3000000 console=ttyS0 debug memtest=0 root=/dev/mmcblk0p1 rootwait rootfstype=ext2";
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x00 0x80000000 0x00 0x2000000>;
	};

	cpus {
		#address-cells = <0x01>;
		#size-cells = <0x00>;
		timebase-frequency = <10000>;

		cpu@0 {
			clock-frequency = <40000000>;
			phandle = <0x01>;
			device_type = "cpu";
			reg = <0x00>;
			status = "okay";
			compatible = "riscv";
			riscv,isa = "rv32ima";
			mmu-type = "riscv,none";

			interrupt-controller {
				#interrupt-cells = <0x01>;
				interrupt-controller;
				compatible = "riscv,cpu-intc";
				phandle = <0x02>;
			};
		};

		cpu-map {

			cluster0 {

				core0 {
					cpu = <0x01>;
				};
			};
		};
	};

	soc {
		#address-cells = <0x02>;
		#size-cells = <0x02>;
		compatible = "simple-bus";
		ranges;

    uart0: uart@10000000 {
      device_type = "serial";
			clock-frequency = <0x1000000>;
			reg = <0x00 0x10000000 0x00 0x100>;
			compatible = "ns16850";
      status = "ok";
		};

    uart1: uart@10000100 {
      device_type = "serial";
			clock-frequency = <0x1000000>;
			reg = <0x00 0x10000100 0x00 0x100>;
			compatible = "ns16850";
      status = "ok";
		};

    uart2: uart@10000200 {
      device_type = "serial";
			clock-frequency = <0x1000000>;
			reg = <0x00 0x10000200 0x00 0x100>;
			compatible = "ns16850";
      status = "disabled";
		};

    uart3: uart@10000300 {
      device_type = "serial";
			clock-frequency = <0x1000000>;
			reg = <0x00 0x10000300 0x00 0x100>;
			compatible = "ns16850";
      status = "disabled";
		};

    uart4: uart@10000400 {
      device_type = "serial";
			clock-frequency = <0x1000000>;
			reg = <0x00 0x10000400 0x00 0x100>;
			compatible = "ns16850";
      status = "disabled";
		};

		poweroff {
			value = <0x5555>;
			offset = <0x00>;
			regmap = <0x04>;
			compatible = "syscon-poweroff";
		};

		reboot {
			value = <0x7777>;
			offset = <0x00>;
			regmap = <0x04>;
			compatible = "syscon-reboot";
		};

		syscon@11100000 {
			phandle = <0x04>;
			reg = <0x00 0x11100000 0x00 0x1000>;
			compatible = "syscon";
		};

		clint@11000000 {
			interrupts-extended = <0x02 0x03 0x02 0x07>;
			reg = <0x00 0x11000000 0x00 0x10000>;
			compatible = "sifive,clint0\0riscv,clint0";
		};

    kianvgpio: gpio@10000700 {
        compatible = "kianV,kianv-gpio";
        reg = <0x00 0x10000700 0x00 0x1000>;
        gpio-controller;
        #gpio-cells = <2>;
    };

		kianVspi0: spi@10500000 {
			compatible = "kianVfpga,kianVspi-simple";
			reg = <0x00 0x10500000 0x00 0x100>;
			status = "okay";
			kianVfpga,num-cs = <1>;
			kianVfpga,sck-frequency = <0>; // sd card works only with small values 
      spi-cpha = <0>;
      spi-cpol = <0>;

			#address-cells = <1>;
			#size-cells    = <0>;

// 		flash: flash@0 {
// 			compatible = "spi-nor";
// 			reg = <0x0>;
//
// 			#address-cells = <0x1>;
// 			#size-cells    = <0x1>;
// 			partition@0 {
// 				label = "spi-nor-flash";
// 				reg = <0x00000000 0x02000000>;
// 			};
//    };

        /*
				partition@0 {
					label = "fpga";
					reg = <0x00000000 0x00020000>;
				};

				partition@20000 {
					label = "bios";
					reg = <0x00020000 0x00010000>;
				};

				partition@30000 {
					label = "dt";
					reg = <0x00030000 0x00010000>;
				};

				partition@40000 {
					label = "kernel";
					reg = <0x00040000 0x005c0000>;
				};

				partition@600000 {
					label = "ubi";
					reg = <0x00600000 0x00a00000>;
				};
		   };
       */

//			spidev0: spidev@0 {
//				//compatible = "linux,spidev";
//				compatible = "kianVfpga,kianVspi-simple";
//				reg = <0>;
//				spi-max-frequency = <15000000>;
//				status = "okay";
//			 };

      mmc-slot@0 {
        compatible = "mmc-spi-slot";
        //bus-width = <1>;
        spi-max-frequency = <24000000>; /* 24 MHz max freq */
        voltage-ranges = <3200 3400>; /* Voltage range in millivolts */
        reg = <0>; /* Chip Select 0 */
        status = "okay";
        disable-wp;
        non-removable;
        no-sdio;
        no-mmc;
      };
		 };
	};
};
